<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
xmlns:app="http://schemas.android.com/apk/res-auto"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".MainActivity"
android:background="@android:color/white">

<ImageView
    android:src="@drawable/groupe"
    android:layout_width="match_parent"
    android:layout_height="@dimen/hauteur_champs_saisie"
    android:scaleType="center"
    android:background="@color/fond1"
    android:contentDescription="@string/app_name" />

<ListView
    android:id="@+id/liste_membres"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:cacheColorHint="#00000000"
    android:layout_marginTop="@dimen/hauteur_champs_saisie"/>

<android.support.design.widget.FloatingActionButton
    android:id="@+id/FabAjoutMembre"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:src="@drawable/fab_ic_add"
    android:layout_alignParentRight="true"
    android:layout_alignParentBottom="true"
    android:layout_marginBottom="@dimen/fab_button_margin_bottom"
    android:layout_marginRight="@dimen/fab_button_margin_right"
    app:elevation="6dp"
    app:pressedTranslationZ="12dp"
    app:borderWidth="0dp"/>

</RelativeLayout>